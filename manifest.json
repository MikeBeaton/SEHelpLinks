{
  "name": "Stack Exchange Help Links",
  "short_name": "SEHelpLinks",
  "version": "0.6.0",
  "description": "Adds useful help links to Stack Exchange and Stack Overflow sites.",
  "homepage_url": "https://github.com/MikeBeaton/SEHelpLinks",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://stackoverflow.com/*",
        "https://*.stackoverflow.com/*",
        "https://stackexchange.com/*",
        "https://*.stackexchange.com/*",
        "https://superuser.com/*",
        "https://*.superuser.com/*",
        "https://serverfault.com/*",
        "https://*.serverfault.com/*",
        "https://askubuntu.com/*",
        "https://*.askubuntu.com/*",
        "https://stackapps.com/*",
        "https://*.stackapps.com/*"
      ],
      "js": ["add-links.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "48": "Help-Green-48x48.jpg",
    "128": "Help-Green-128x128.jpg"
  }
}
