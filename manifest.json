{
  "name": "Stack Exchange Help Links",
  "short_name": "SEHelpLinks",
  "version": "0.4.0",
  "description": "Adds useful help links to Stack Exchange and Stack Overflow sites. Icon courtesy of https://icons8.com .",
  "homepage_url": "https://github.com/MikeBeaton/SEHelpLinks",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://stackoverflow.com/*",
        "https://*.stackoverflow.com/*",
        "https://stackexchange.com/*",
        "https://*.stackexchange.com/*",
        "https://superuser.com/*",
        "https://*.superuser.com/*",
        "https://serverfault.com/*",
        "https://*.serverfault.com/*"        
      ],
      "js": ["add-links.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "48": "Help-48x48.png",
    "128": "Help-128x128.png"
  }
}
